[match-node]
properties = [
  { name = "media.class", value = "Audio/Sink" },
  { name = "device.api", value = "alsa" },
  { name = "api.alsa.pcm.id", value = "USB Audio" },
]

[endpoint]
session = "audio"
type = "si-adapter"
streams = "audio-sink.streams"

[endpoint.config]
priority = 5
enable-monitor = true
