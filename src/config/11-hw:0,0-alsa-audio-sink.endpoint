[match-node]
properties = [
  { name = "media.class", value = "Audio/Sink" },
  { name = "device.api", value = "alsa" },
  { name = "api.alsa.path", value = "hw:0,0" },
]

[endpoint]
session = "audio"
type = "si-adapter"
#streams = "audio-sink.streams"

[endpoint.config]
priority = 1
enable-monitor = true
